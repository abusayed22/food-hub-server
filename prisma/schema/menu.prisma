
model menu {
  id          String  @id @default(cuid())
  name        String
  isAvailable Boolean @default(true)
  price       Float
  description String?
  iamge       String?
  category    String[]

  review      review[]
  user_id String
  createdAt   DateTime   @default(now())
  updateAt    DateTime   @default(now()) @updatedAt
}
